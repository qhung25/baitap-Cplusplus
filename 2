#include<stdio.h>
int main()
{
	int km,money;
	printf("Nhap so km: ");scanf("%d", &km);
	if(km<2)
	{
		money = km*15000;
	}
	else if(km>=2 && km <6)
	{
		money = km*13500;
	}
	else if(km>=6)
	{
		money = km*11000;
	}
	if(km > 120)
	{
		money -= (10*money)/100;
	}
	printf("Voi so km la %d thi tong so tien can tra kla: %d", km, money);
	
}
